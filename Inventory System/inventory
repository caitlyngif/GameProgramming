// added to player
using UnityEngine;

public class Inventory : MonoBehavior{
    List<EqEnum> itmes = new();
    [SerializeField] List<InventorySlot> slots = new();
    int available;

    internal void Add(EqEnum key){
        if (available < slots.Count){
            items.Add(key)
            slots[available++].Set(key);
        }
        else{
            Debug.Log("Inventory is full")
        }
    }
}